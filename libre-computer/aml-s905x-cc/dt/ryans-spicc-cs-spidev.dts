/*
 * Overlay aimed to enable a spidev interface on SPICC bus with two CS PINS 38, 40
 */

/dts-v1/;
/plugin/;

/ {
	compatible = "libretech,cc", "amlogic,s905x", "amlogic,meson-gxl";

	fragment@0 {
		target = <&spicc>;
		
		__overlay__ {
			
			spidev@2 {
				compatible = "rohm,dh2228fv";
				reg = <2>;
				spi-max-frequency = <30000000>;
			};
			spidev@3 {
				compatible = "rohm,dh2228fv";
				reg = <3>;
				spi-max-frequency = <30000000>;
			};
			
		};
	};
};
